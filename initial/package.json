{
  "name": "restatets",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "prettier": "npx prettier --write src/**/*.{ts,tsx,css,scss,html,json,js}",
    "build": "npx rollup -c rollup.config.js",
    "build-watch": "npx rollup -w -c rollup.config.js",
    "test": "npx jest",
    "test-watch": "npx jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "json": "^10.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/src/**/*.spec.ts"
    ]
  }
}
